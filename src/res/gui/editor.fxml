<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>


<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.CodeEditor">
   <children>
      <HBox stylesheets="@editor.css">
         <children>
            <TextArea fx:id="lineArea" editable="false" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="40.0" styleClass="lineArea" />
            <TextArea fx:id="codeArea" onKeyPressed="#handleKeyPressed" prefHeight="200.0" prefWidth="300.0" styleClass="codeArea" />
         </children>
      </HBox>
   </children>
</Pane>
